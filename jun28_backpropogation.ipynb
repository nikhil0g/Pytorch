   "source": [
    "import torch\n",
    "\n",
    "x = torch.tensor(1.0)\n",
    "y = torch.tensor(2.0)\n",
    "\n",
    "w = torch.tensor(1.0, requires_grad=True)\n",
    "\n",
    "#forword pass and compute loss\n",
    "\n",
    "yhat = w * x\n",
    "loss = (yhat - y)**2\n",
    "\n",
    "print(loss)\n",
    "\n",
    "#Backward pass\n",
    "loss.backward()\n",
    "print(w.grad)\n"
   ]
  },
